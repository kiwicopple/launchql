    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "4.0.6",
    "jest": "^25.1.0",
    "prettier": "^2.0.5",
    "regenerator-runtime": "^0.13.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.4",
    "@launchql/db-env": "^0.1.0",
