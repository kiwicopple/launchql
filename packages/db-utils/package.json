    "@launchql/db-transform": "^0.2.0",
    "envalid": "6.0.2",
    "fuzzy": "0.1.3",
    "glob": "7.1.6",
    "mkdirp": "^1.0.4",
    "node-walkup": "^1.1.1",
    "pg": "^8.3.3",
    "pgsql-parser": "0.7.9",
    "rimraf": "^3.0.2",
    "semver": "^5.5.1",
    "shelljs": "^0.8.2"
